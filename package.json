{
  "name": "vanilla-commons",
  "version": "1.1.0",
  "description": "Lightweight common vanilla utilities for the modern web development",
  "main": "dist/vanilla-commons.js",
  "module": "dist/vanilla-commons.es.js",
  "jsnext:main": "dist/vanilla-commons.es.js",
  "browser": "dist/vanilla-commons.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prelint": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"lib/**/*.js\" --write build.js --write \"test/**/*.js\"",
    "lint": "xo --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls",
    "build": "node build.js",
    "precommit": "lint-staged && npm test && npm run build",
    "prepublish": "npm run precommit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiamsantos/vanilla-commons.git"
  },
  "keywords": [
    "util",
    "functional",
    "immutable",
    "pure",
    "modules",
    "lightweight",
    "library",
    "fast",
    "small",
    "common",
    "utilities",
    "vanilla",
    "modern",
    "curry"
  ],
  "author": "Thiago Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiamsantos/vanilla-commons/issues"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ]
  },
  "homepage": "https://github.com/thiamsantos/vanilla-commons#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.2.3",
    "prettier": "^1.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "envs": [
      "browser",
      "jest"
    ],
    "rules": {
      "no-mixed-operators": "warn",
      "import/prefer-default-export": 0
    },
    "space": true,
    "semicolon": false
  },
  "dependencies": {
    "is-boolean-attribute": "0.0.1"
  },
  "lint-staged": {
    "*.js": ["npm run lint", "git add"]
  }
}
